{
  "expo": {
    "name": "Buzo AI",
    "slug": "buzo-ai",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "jsEngine": "hermes",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#4F46E5"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.buzoai.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.buzoai.app",
      "versionCode": 9,
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.CAMERA",
        "android.permission.INTERNET",
        "com.android.vending.BILLING"
      ]
    },
    "web": {},
    "plugins": [
      "expo-notifications",
      "expo-camera",
      "expo-secure-store"
    ],
    "extra": {
      "eas": {
        "projectId": "383659eb-063a-4e25-a579-d860cbc1bf36"
      }
    },
    "owner": "sofoalbert"
  }
}
